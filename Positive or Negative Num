#include <stdio.h>
int main()
{
  //INPUT
  int num;
  printf("Enter a num:");
  scanf("%d",&num);
  // Positive or Negative
  if(num>0)
  {
      
      printf("The number is positive");
  }
  else if(num==0)
  {
      printf("The number is neither positive nor negative");
  }
  else
  {
      printf("The number is negative");
  }
  
}
    

