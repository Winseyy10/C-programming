#include <stdio.h>
int main()
{
  //INPUT
    int year;
    printf("Enter a year:");
    scanf("%d",&year);
    
    //leap year
    if(year%100==0)
{
  if(year%400==0)
  {
    printf("The year is leap year");
  }
else
{
  printf("The year is not a leap year");
}

}
else
{
  if(year%4 ==0)
  {
    printf("year is leap year");
  }
}

}

    
